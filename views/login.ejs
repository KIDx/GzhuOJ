<div class="jqmSubmit" id="logindialog" style="width:400px;height:250px;top:25%;left:35%;">
  <div align="center">
    <div class="bread jqDrag jqtop" style="width:390px;">
      Login
    </div>
  </div>
  <div class="jqcontent" style="width:390px;height:160px;padding-left:10px;">
    <div>
      <label for="login_username">User Name *</label>
      <input type="text" class="jqtext" style="width:370px;" id="login_username">
    </div>
    <div>
      <label for="login_password">Password *</label>
      <input type="password" class="jqtext" style="width:370px;" id="login_password">
    </div>
    <small id="login_error" class="error-text"></small>
  </div>
  <div class="jqfoot">
    <a class="uibtn" href="javascript:;" id="login_submit">Login</a>
    <a class="uibtn loginclose" href="javascript:;">Cancel</a>
  </div>
  <div class="jqResize"></div>
  <i class="loginclose jqclose"></i>
</div>