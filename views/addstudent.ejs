<!--<html>-->
<% var cen = parseInt((new Date()).getFullYear(), 10)%100; %>
<% var g = cen, c = 1; %>
<fieldset>
	<div>
		<legend>批量注册</legend>
	</div>
	<div>
		<span class="uibtn upload-btn">
			<span>选择文件</span>
			<input type="file" id="file" name="csv" data-url="/csvUpload" />
		</span>
		<span id="upload-info" class="upload-info">未选择文件 (支持csv格式)</span>
	</div>
	<div>
		<a class="uibtn" id="upload" href="javascript:;">开始上传</a>
		<span class="error-text" id="err"></span>
	</div>
	<div id="table" class="tablebg hide" style="margin-top:20px;">
		<div id="pager">
			<div class="fl" id="filter">
				<div>设置：</div>
				<select style="width:220px;"  id="college">
				<% for (var i = 0; i < C.length; i++) { %>
					<option value="<%= i %>"<% if (i == 1) { %> selected<% } %>>
						<%= C[i] %>
					</option>
				<% } %>
				</select>
				<select style="width:50px;" id="grade">
				<% for (var i = cen-10; i <= cen; i++) { %>
					<% var v = i; if (i < 10) { v = '0'+i; } %>
					<option value="<%= v %>" <% if (g == i) { %>selected<% } %>><%= v %></option>
					<% } %>
				</select>&nbsp;级
				<select style="width:50px;" id="class">
					<% for (var i = 1; i < 10; i++) { %>
					<option value="<%= i %>" <% if (i == c) { %>selected<% } %>><%= i %></option>
					<% } %>
				</select>&nbsp;班
			</div>
			<div class="fr" id="filter">
				<a id="submit" class="uibtn uibtn-mini" href="javascript:;">开始批量注册</a>
			</div>
		</div>
		<div id="tablediv">
			<table class="table table-cf tdcenter">
				<thead>
					<tr>
						<th style="width:20%;">学号 (用户名)</th>
						<th style="width:20%;">姓名</th>
						<th style="width:20%;">性别</th>
						<th style="width:20%;">操作</th>
						<th style="width:20%;">状态</th>
					</tr>
				</thead>
				<tbody id="tbody">
				</tbody>
			</table>
		</div>
	</div>
</fieldset>